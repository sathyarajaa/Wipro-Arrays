import java.util.*;
public class Present1Or4 {
	public static void main(String args[]) {
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		int arr[]=new int[n];
		for(int i=0;i<n;i++) {
			arr[i]=sc.nextInt();
		}
		int c=0;
		for(int i=0;i<n;i++){
		if(arr[i]!=4 && arr[i]!=1){
			c=1;
		    break;
		}
		}
		if(c==0)
			System.out.println("True");
		else
			System.out.println("False");
		}
}
