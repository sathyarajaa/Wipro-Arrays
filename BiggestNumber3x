import java.util.Scanner;
public class BiggestNumber3x3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc=new Scanner(System.in);
		int arr[][]=new int[3][3];
		int max=0;
		for(int i=0;i<3;i++) {
			for(int j=0;j<3;j++) {
				arr[i][j]=sc.nextInt();
				if(max<arr[i][j])
					max=arr[i][j];
				
			}
		}
		System.out.print(max);
			
			

	}

}
